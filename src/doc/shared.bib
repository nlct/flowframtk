% Encoding: UTF-8

% ABBREVIATIONS

@abbreviation{gui,
  short="GUI",
  long="graphical user interface",
  description = {An application with windows and buttons in which the user
      can point and click with the mouse.}
}

@commonabbreviation{pdf,
  short = {PDF},
  long = {portable document format}
}

@commonabbreviation{html,
  short = {HTML},
  long = {hypertext markup language}
} 
  
@commonabbreviation{xml,
  short = {XML},
  long = {extensible markup language}
} 

@entry{ajr,
  name="AJR",
  description = {FlowframTk's native ASCII format. Files in this format 
      should have the extension \ext{ajr}. This format is 
      primarily provided to assist conversion to and from \gls{jdr} files 
      or for version control.}
}

@entry{jdr,
   name="JDR",
   description = {FlowframTk's native binary format. Files in this format 
      should have the extension \ext{jdr}.}
}


% FILE EXTENSIONS/FORMATS

@indexplural{fileextension, name={file extension}}

@fileext{ext.jdr, parent={fileextension}}
@fileext{ext.ajr, parent={fileextension}}
@fileext{ext.jar, parent={fileextension}}

% APPLICATIONS

@application{app.flowframtk,
  name={\appfmt{flow\-fram\-tk}},
  syntax={\meta{options} \meta{filename} \ldots\ \meta{filename}}
}

@application{app.jdrview,
  name={\appfmt{jdr\-view}},
  syntax={\meta{options} \meta{filename}}
}



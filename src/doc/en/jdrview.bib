% Encoding: UTF-8


% SWITCHES

@switch{switch.help,
  parent={app.jdrview},
  shortswitch={h},
  description={Prints help and exits}
}

@switch{switch.version,
  parent={app.jdrview},
  shortswitch={v},
  description={Prints version information and exits}
}
  
@switch{switch.antialias,
  parent={app.jdrview},
  description={Switch on the anti-aliasing}
}

@switch{switch.noantialias,
  parent={app.jdrview},
  description={Switch off the anti-aliasing}
}

@switch{switch.cwd,
  parent={app.jdrview},
  syntax={\meta{path}},
  description={Set the current working directory to \meta{path}}
}


@file{file.jdrview.jar}
@file{file.jdrview}
